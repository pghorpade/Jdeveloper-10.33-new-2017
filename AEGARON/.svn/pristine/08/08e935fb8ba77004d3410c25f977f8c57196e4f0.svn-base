package aegaron.service;

public class DrawingMetadata {
    private String altDrawingID;
    private String title;
    private String altPlanTitle;
    private String printSizeType;
    private String idealType;
    private String coverageGeoscale;
    
    @Override
    public String toString() {
        return "DrawingMetadata [title=" + title + 
                ", altPlanTitle=" + altPlanTitle + 
                ", altDrawingID=" + altDrawingID + 
                ", printSizeType=" + printSizeType + 
                ", idealType=" + idealType + 
                ", coverageGeoscale='" + coverageGeoscale + 
                ']';
    }
    
    public void setTitle(String title) {
        this.title = title;
    }

    public String getTitle() {
        return title;
    }

    public void setAltDrawingID(String altDrawingID) {
        this.altDrawingID = altDrawingID;
    }

    public String getAltDrawingID() {
        return altDrawingID;
    }

    public void setAltPlanTitle(String altPlanTitle) {
        this.altPlanTitle = altPlanTitle;
    }

    public String getAltPlanTitle() {
        return altPlanTitle;
    }

    public void setPrintSizeType(String typePrintSize) {
        this.printSizeType = typePrintSize;
    }

    public String getPrintSizeType() {
        return printSizeType;
    }

    public void setIdealType(String typeIdeal) {
        this.idealType = typeIdeal;
    }

    public String getIdealType() {
        return idealType;
    }

    public void setCoverageGeoscale(String coverageGeoscale) {
        this.coverageGeoscale = coverageGeoscale;
    }

    public String getCoverageGeoscale() {
        return coverageGeoscale;
    }
}
