#!/bin/sh
#=========================================================================== 
# JDeveloper launch script (Debug)
#=========================================================================== 
cd `dirname $0`

JDEV_VM_OPTIONS='-classic -J-Xdebug -J-Xnoagent -J-Djava.compiler=NONE -J-Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5000'

./jdev ${JDEV_VM_OPTIONS}

